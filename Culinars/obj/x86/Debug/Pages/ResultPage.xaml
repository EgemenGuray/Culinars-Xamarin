<Page
    x:Class="Culinars.ResultPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Culinars"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Page.Transitions>
        <TransitionCollection>
            <EdgeUIThemeTransition Edge="Right"></EdgeUIThemeTransition>
        </TransitionCollection>
    </Page.Transitions>


    <Grid Background="Transparent">
        <Grid.RowDefinitions>


            <RowDefinition Height="*"/>
            <RowDefinition Height="10*"/>




        </Grid.RowDefinitions>

        <TextBlock Margin="0,0,100,0" Text="Matches" VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="25" FontWeight="ExtraLight" ></TextBlock>
        <Button x:ConnectionId='1' x:Name="HamburgerButton" FontFamily="Segoe MDL2 Assets" Content="&#xE0A6;"
                    Width="50" Height="50" Background="Transparent"                     />

        <GridView x:ConnectionId='2'                                              x:Name="GridView" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,0,100,0" ScrollViewer.VerticalScrollBarVisibility="Hidden" >
            


        </GridView>




   

       </Grid>



</Page>

